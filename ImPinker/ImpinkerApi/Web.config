  <log4net configSource="config\log4net.config" />
  <connectionStrings>
    <add name="ImPinker" providerName="System.Data.SqlClient" connectionString="Data Source=101.200.175.157;Initial Catalog=MyAutosTest;Persist Security Info=True;User ID=myautos;Password=myautos2016" />
    <add name="DefaultConnection" providerName="System.Data.SqlClient" connectionString="Data Source=(LocalDb)\v11.0;Initial Catalog=aspnet-ImpinkerApi-20160627145841;Integrated Security=SSPI;AttachDBFilename=|DataDirectory|\aspnet-ImpinkerApi-20160627145841.mdf" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--<add key="SolrServer" value="http://101.200.175.157:8080/solr/impinkerTest" />-->
    <add key="SolrServer" value="http://127.0.0.1:8080/solr/" />
    <!--缓存时间。分钟-->
    <add key="ModelCache" value="30"/>
    <!--车酷oss图片根目录-->
    <add key="MyautosOssBucket" value="myautos" />
    <!--用户头像地址，通过oss读取图片(3钟规格)-->
    <add key="HeadImageLimit" value="headimg/limit/{0}/{1}_{2}.jpg" />
    <!--用户发微博图片保存路径-->
    <add key="WeiboImage" value="weiboimage/{0}/{1}_{2}.jpg" />
    <!--用户发游记，编辑游记的时候上传的图片路径-->
    <add key="ArticleImage" value="articleimage/{0}/{1}_{2}_{3}.jpg" />
    <!--游记、文章封面图路径-->
    <add key="ArticleFirstImage" value="articlefirstimg/{0}/{1}_{2}_{3}.jpg" />
    <!--oss img 读取的根路径-->
    <add key="ImageDomain" value="http://img.myautos.cn/" />
    <!--阿里大鱼短信验证模板-->
    <add key="SmsTemplateCode" value="SMS_36830017"/>
